	<isChildOf xmlns="http://purl.org/gem/qualifiers/" rdf:resource="http://vocabulary.curriculum.edu.au/MRAC/2023/07/LA/MAT/fea51b3d-d9a9-4795-b83f-66195789049b"/>
	<nominalYearLevel xmlns="https://www.esa.edu.au/" xml:lang="en-au">Year 9</nominalYearLevel>
</rdf:Description>


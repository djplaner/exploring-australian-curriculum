<rdf:Description rdf:about="http://vocabulary.curriculum.edu.au/MRAC/2023/07/LA/MAT/21ec0e04-b342-4210-8dd9-7f98c13bb7d3">
	<rdf:type rdf:resource="http://purl.org/ASN/schema/core/Statement"/>
